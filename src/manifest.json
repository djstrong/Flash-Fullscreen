{
  "name": "Flash Fullscreen",
  "version": "0.1.1",
  "description": "Allows to make any flash object (e.g. game) fullscreen.",
  "manifest_version": 2,
  "icons": { "128": "sample-128.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "js" : ["inj.js"],
      "all_frames": true
    }
  ]
}
